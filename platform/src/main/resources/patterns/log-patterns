TIMESTAMP %{DATE} %{TIME}
MAIN_CLASS (?:\[\w+\])
GENERIC_CLASS %{USERNAME}
CLASS %{MAIN_CLASS}|%{GENERIC_CLASS}
SYSLOGFACILITY <%{NONNEGINT}.%{NONNEGINT}>
SYSBASE %{SYSLOGTIMESTAMP:Timestamp} (?:%{SYSLOGFACILITY} )?%{SYSLOGHOST:Logsource}
SYSLOG %{SYSBASE} %{GREEDYDATA:Message}
HADOOP %{TIMESTAMP:Timestamp} %{LOGLEVEL:Level} %{SPACE} %{CLASS:Class} %{GREEDYDATA:Message}
DATANODELOG %{HADOOP}
HBASELOG %{HADOOP}
YARNLOG %{HADOOP}
